@model IEnumerable<Order>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Заказы</h2>
<table class="table">
    <tr>
        <th>Id заказа</th> 
        <th>Товар</th>
        <th>Клиент</th>
        <th>Адрес</th>
        <th>Сумма</th>
        <th>Дата заказа</th>
    </tr>
    @foreach (var t in @Model)
    {
        <tr>
            <td>@t.Id</td> 
            <td>@t.Product.Name</td> 
            <td>@t.CustomerName</td>
            <td>@t.Address</td>  
            <td>@t.Product.Price</td>
            <td>@t.Date.ToShortDateString()</td>
            
        </tr>
    }
</table>